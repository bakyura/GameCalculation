<div class="matches_det">  <div>    <div class="f_left">      <h1>Football Matche</h1>    </div>    <div class="clear"></div>    <div class="title">      <div class="f_left">        <%= image_tag "/images/#{@league.logo}", :height => "85" %>      </div>      <div class="name">        <%= @league.name %>      </div>      <div class="tour">        <%= @tour %> tour      </div>    </div>    <div class="matche_det" style="height: auto;">      <div class="matche_det_date">Date: <%= @matche.game_date %></div>      <div class="matche_det_name">        <div class="f_left">          <%= image_tag "/images/#{@team1.logo}", :height => "85" %>        </div>        <div class="f_left teams_container">          <%= @team1.name %> - <%= @team2.name %>        </div>        <div class="f_left">          <%= image_tag "/images/#{@team2.logo}", :height => "85" %>        </div>      </div>      <div id="matche_det_info">        <br/>        <br/>        <div class="matche_det_info_item">          <% @prediction = Prediction.first(:conditions => {:game_id => @matche.id}) %>          <% unless @prediction.nil? %>            <b>Our forecast for the game:</b><br />            Win Team 1: <span class="matche_info_item_r1"><%= @prediction.win_home %>%</span><br />            Draw: <span class="matche_det_info_item_rx"><%= @prediction.draw_result %>%</span><br />            Win Team 2: <span class="matche_det_info_item_r2"><%= @prediction.win_quest %>%</span><br />            Possible game result: <span class="matche_det_info_item_r1"><%= @prediction.goals_home %>:<%= @prediction.goals_quest %></span><br />            <% if @prediction.is_recommended %>              <span class="matche_det_info_item_r2">We recommend this match for bet!</span>              <br />            <% end %>            <% unless @prediction.comment.nil? %>              <br />              <b>More forecasts:</b><br/>              <%== @prediction.comment %>            <% end %>          <% end %>        </div>        <br />        <br />        <div class="matche_det_info_item">          <% @prediction = BettingPrediction.first(:conditions => {:game_id => @matche.id, :bookmaker_id => @will_hill.id}) %>          <% unless @prediction.nil? %>            <b>WillHill</b><br />            Win Team 1: <span class="matche_info_item_r1"><%= @prediction.r_1 %></span><br />            Draw: <span class="matche_info_item_rx"><%= @prediction.r_x %></span><br />            Win Team 2: <span class="matche_info_item_r2"><%= @prediction.r_2 %></span><br />          <% end %>        </div>        <br />        <br />        <div class="matche_det_info_item">          <% @prediction = BettingPrediction.first(:conditions => {:game_id => @matche.id, :bookmaker_id => @bet_365.id}) %>          <% unless @prediction.nil? %>            <b>Bet365</b><br />            Win Team 1: <span class="matche_det_info_item_r1"><%= @prediction.r_1 %></span><br />            Draw: <span class="matche_det_info_item_rx"><%= @prediction.r_x %></span><br />            Win Team 2: <span class="matche_det_info_item_r2"><%= @prediction.r_2 %></span><br />          <% end%>        </div>        <br />        <br />        <div class="matche_det_info_item">          <% @prediction = BettingPrediction.first(:conditions => {:game_id => @matche.id, :bookmaker_id => @unibet.id}) %>          <% unless @prediction.nil? %>            <b>Unibet</b><br />            Win Team 1: <span class="matche_det_info_item_r1"><%= @prediction.r_1 %></span><br />            Draw: <span class="matche_det_info_item_rx"><%= @prediction.r_x %></span><br />            Win Team 2: <span class="matche_det_info_item_r2"><%= @prediction.r_2 %></span><br />          <% end %>          </div>        <br />        <br />        <div class="matche_det_info_item">          <b>Games history:</b><br/>          Coming soon        </div>        <br />        <br />        <div class="matche_det_info_item">          <b>Weather forecast:</b><br />          Coming soon        </div>        <br />        <br />        <div class="matche_det_info_item">          <b>Latest news:</b><br />          Coming soon        </div>        <br />        <br />        <div class="matche_det_info_item">          <b>Text<span class="red">Live:</span></b><br />          Coming soon        </div>        <br />        <br />        <div class="matche_det_info_item">          <b>Video<span class="red">Live:</span></b><br />          Coming soon        </div>        <br />        <br />      </div>    </div>  </div></div>