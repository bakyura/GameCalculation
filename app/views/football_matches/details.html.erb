<div class="matches">  <div>    <div class="f_left">      <h1>Football Matche</h1>    </div>    <div class="clear"></div>    <div class="title">      <div class="f_left">        <%= image_tag "/images/#{@league.logo}", :height => "100" %>      </div>      <div class="name">        <%= @league.name %>      </div>      <div class="tour">        <%= @tour %> tour      </div>    </div>    <br />    <br />    <br />    <div class="matche">      <div class="matche_name">        <div class="f_left">          <%= image_tag "/images/#{@team1.logo}", :height => "100" %>        </div>        <div class="f_left teams_container">          <%= @team1.name %> - <%= @team2.name %>        </div>        <div>          <%= image_tag "/images/#{@team2.logo}", :height => "100" %>        </div>      </div>    </div>  </div></div>